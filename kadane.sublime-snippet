	<content><![CDATA[
	ll kadane(vector<ll> v)
	{
		ll sum = 0 , ans = 0;
		for(auto i : v)
		{
			sum += i;
			ans = max(ans,sum);
			if(sum < 0) sum = 0;
		}
		return ans;
	}
]]></content>
